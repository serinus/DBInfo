                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT          SYS.USER_TAB_COMMENTS.TABLE_NAME, 
                      SYS.USER_TAB_COMMENTS.TABLE_TYPE, 
                      SYS.USER_TAB_COMMENTS.COMMENTS AS TAB_COMMENTS, 
                      SYS.USER_COL_COMMENTS.COLUMN_NAME, 
                      SYS.USER_COL_COMMENTS.COMMENTS AS COL_COMMENTS, 
                      CONCAT(CONCAT(SYS.USER_TAB_COMMENTS.COMMENTS, '．'), 
                      SYS.USER_COL_COMMENTS.COMMENTS) AS TAB_COL_COMMENTS, 
                      CONCAT(CONCAT(CONCAT(SYS.USER_TAB_COMMENTS.COMMENTS, '('), 
                      SYS.USER_TAB_COMMENTS.TABLE_NAME), ')') AS TABLE_NAME_COMMENTS, 
                      CONCAT(CONCAT(CONCAT(SYS.USER_COL_COMMENTS.COMMENTS, '('), 
                      SYS.USER_COL_COMMENTS.COLUMN_NAME), ')') 
                      AS COLUMN_NAME_COMMENTS
FROM            SYS.USER_TAB_COMMENTS, SYS.USER_COL_COMMENTS, 
                      SYS.USER_TAB_COLUMNS
WHERE           SYS.USER_TAB_COMMENTS.TABLE_NAME = SYS.USER_COL_COMMENTS.TABLE_NAME
                       AND 
                      SYS.USER_COL_COMMENTS.TABLE_NAME = SYS.USER_TAB_COLUMNS.TABLE_NAME
                       AND 
                      SYS.USER_COL_COMMENTS.COLUMN_NAME = SYS.USER_TAB_COLUMNS.COLUMN_NAME
                       AND (SYS.USER_TAB_COMMENTS.TABLE_NAME LIKE CONCAT(CONCAT('%', 
                      :TABLE_NAME), '%')) AND 
                      (SYS.USER_COL_COMMENTS.COLUMN_NAME LIKE CONCAT(CONCAT('%', 
                      :COLUMN_NAME), '%')) AND 
                      (SYS.USER_TAB_COMMENTS.TABLE_TYPE LIKE CONCAT(CONCAT('%', 
                      :TABLE_TYPE), '%')) AND 
                      (SYS.USER_TAB_COMMENTS.COMMENTS LIKE CONCAT(CONCAT('%', 
                      :TAB_COMMENTS), '%')) AND 
                      (SYS.USER_COL_COMMENTS.COMMENTS LIKE CONCAT(CONCAT('%', 
                      :COL_COMMENTS), '%'))
ORDER BY     SYS.USER_TAB_COLUMNS.TABLE_NAME, SYS.USER_TAB_COLUMNS.COLUMN_ID</CommandText>
